{
  "name": "capacitor-jitsi-meet",
  "version": "1.5.0",
  "description": "This plugin is used to make video calls using Jitsi video platform (https://meet.jit.si) on iOS and Android using Capacitor.",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Calvin Ho",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "^2.4.1"
  },
  "devDependencies": {
    "typescript": "^2.9.2"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorJitsiMeet.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/calvinckho/capacitor-jitsi-meet"
  },
  "bugs": {
    "url": "https://github.com/calvinckho/capacitor-jitsi-meet/issues"
  }
}
